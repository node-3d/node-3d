{
    "rootDir": "../",
    "moduleFileExtensions": [
        "js",
        "node"
    ],
    "testMatch": [
        "<rootDir>/**/*.test.js"
    ],
    "coverageDirectory": "doc/jest",
    "coverageReporters": [
        "lcov"
    ],
    "collectCoverageFrom": [
        "**/*.js",
        "!**/*.test.js"
    ]
}
